<ul class="flex-container">
  <li class="flex-item">
	<h1 id="category_name"><%=@category.name%></h1>

  <li class="flex-item"><h1 id="summary">
	<%= @summary %>
	<br><br>
	<div id="wikipedia_attribution"><%= link_to 'source: Wikipedia', "#{lookup_wikipedia(@category)}" %>
</div></li>


	<ul><% @category.contents.each do |content| %>
		<% unless content.url == nil %>
			<li class="container"><%= link_to content.url, content.url %></li>
		<% end %>
	<% end %></ul>

</li>


  <li class="flex-item"><canvas id="viewport" width="600" height="600"><br></canvas></li>
</ul>

