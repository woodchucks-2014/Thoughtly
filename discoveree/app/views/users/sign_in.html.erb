<div id="fade">
</div>

<%= form_for :user,  url: {action: "login"} do |f| %>
	<h3>welcome, thinker</h3><br>
    <%= f.text_field :email, placeholder: "Email" %><br><br>
    <%= f.password_field :password, placeholder: "Password"  %><br><br>
      <button id="button"><%= f.submit "Sign Me In!"%></button><br>
    <%= link_to 'or create a new account', users_signup_path %><br><br>
<% end %>
